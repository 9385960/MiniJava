/**
 * COMP 520
 * Array update and array.length
 */
class MainClass {
   public static void main (String [] args) {

      A test = new A();
      test.num = 65;
      System.out.println(65);
      System.out.println(10);
      System.out.println(test.num);
      System.out.println(10);
   }
}

class A {
   int num;
}